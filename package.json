{
    "main": "dist/commands/startGCPHandler.js",
    "dependencies": {
        "@google-cloud/firestore": "^2.2.1",
        "@google-cloud/functions-framework": "^3.3.0",
        "@types/lodash": "^4.14.123",
        "@types/minimist": "^1.2.0",
        "@types/moment-timezone": "^0.5.30",
        "@types/node": "^11.13.2",
        "@types/puppeteer": "^2.0.0",
        "@types/puppeteer-core": "^2.0.0",
        "@types/shortid": "^0.0.29",
        "@types/split2": "^2.1.6",
        "csv-parse": "^4.14.2",
        "enquirer": "^2.3.0",
        "fastify": "^4.10.2",
        "firebase-admin": "^9.5.0",
        "firebase-functions": "^3.13.2",
        "israeli-bank-scrapers-core": "latest",
        "lodash": "^4.17.21",
        "minimist": "^1.2.5",
        "moment": "^2.24.0",
        "moment-timezone": "^0.5.33",
        "proxy-chain": "^2.2.0",
        "puppeteer": "19.8",
        "shortid": "^2.2.14",
        "ts-node": "^8.0.3",
        "tsconfig-paths": "^4.1.1",
        "typescript": "^4.9.3",
        "yn": "^3.1.0",
        "ynab": "^1.49.0"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.109",
        "aws-lambda": "^1.0.7",
        "prettier": "^1.16.4",
        "tslint": "^5.15.0",
        "tslint-config-prettier": "^1.18.0"
    },
    "scripts": {
        "build": "./node_modules/.bin/tsc",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "runCommand": "node --enable-source-maps dist/src/runCommand.js",
        "runCommand:dev": "GOOGLE_APPLICATION_CREDENTIALS=./firebase-service-account.json ts-node --files src/runCommand.ts",
        "scrape": "yarn runCommand scrape",
        "scrape:dev": "yarn runCommand:dev scrape",
        "configure": "yarn runCommand configure",
        "getOtpContext": "yarn runCommand getOtpContext",
        "changeids": "yarn runCommand changeIds",
        "startWeb": "yarn runCommand startWeb",
        "startWeb:dev": "yarn runCommand:dev startWeb",
        "startBot": "yarn runCommand startBot",
        "uploadToYnab": "yarn runCommand uploadToYnab",
        "heroku-postbuild": "echo $GOOGLE_CREDENTIALS | base64 -d > firebase-service-account.json",
        "gcp-build": "yarn build && node node_modules/puppeteer/install.js"
    },
    "optionalDependencies": {
        "telegraf": "^3.30.1"
    },
    "packageManager": "yarn@3.3.0",
    "resolutions": {
        "israeli-bank-scrapers-core/puppeteer-core": "19.8.0"
    }
}
